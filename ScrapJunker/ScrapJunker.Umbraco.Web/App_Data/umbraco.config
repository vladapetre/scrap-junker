      <mstyle displaystyle="true" scriptlevel="0">
        <mn>6.14</mn>
        <mo>&#x22C5;<!-- ⋅ --></mo>
        <mn>7.95</mn>
        <mo>=</mo>
        <mn>48.813</mn>
      </mstyle>
    </mrow>
