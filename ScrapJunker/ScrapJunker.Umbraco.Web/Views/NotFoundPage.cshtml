@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = "MasterPage.cshtml";
}

@section Styles{
<style>
    #404 {
        margin: 0 auto;
    }
</style>
}

<div class="row">
    <div id="404" class="offset-4 col-md-4 col-xs-4 vamiddle">
        <div class="clearfix">
            <h1 class="pull-left display-3 m-r-2">404</h1>
            <h4 class="p-t-1">Oops! You're lost.</h4>
            <p class="text-muted">The page you are looking for was not found.</p>
        </div>
        <div class="input-prepend input-group">
            <span class="input-group-addon"><i class="fa fa-search"></i></span>
            <input id="prependedInput" class="form-control" size="16" type="text" placeholder="What are you looking for?">
            <span class="input-group-btn">
                <button class="btn btn-info" type="button">Search</button>
            </span>
        </div>
        @*<span class="input-group-btn">
            <button class="btn btn-info" type="button"><a href="@CurrentPage.Site().Url">Back to home</a></button>
        </span>*@
    </div>
</div>